.find-page-tj {
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}

.fint-tj-list {
  width: 100%;
  display: flex;
  margin-bottom: 20rpx;
}

.find-tj-imgs {
  width: 190rpx;
  height: 190rpx;
  border-radius: 10px;
}

.find-tj-text {
  width: 500rpx;
  margin-left: 20rpx;
}

.find-tj-title {
  font-size: 30rpx;
  font-weight: bold;
  margin: 20rpx 0;
}

.find-tj-midas {
  font-size: 28rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}