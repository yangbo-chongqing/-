  color: #999;
}
.app-info-tips-item-title{
  width: 400rpx;
  color: #333;
}
.liner{
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
}
.pro-audio{
  display: flex;
  position: relative;
  width: 100%;
  height: 80rpx;
  justify-content: space-between;
  align-items: center;
  background: #5287fd;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 999px;
}
.pro-audio-radios{
  width: 120rpx;
  height: 120rpx;
  border: 2px solid #ea7152;
  border-radius: 50%;
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.pro-audio-play-btn{
  font-size: 60rpx;
  color: #fff;
}
.pro-audio-img,.pro-audio-play-btn{
  display: flex;
  justify-content: center;
  align-items: center;
}
.pro-audio-pross{
  width: 380rpx;
}
.pro-audio-durtime{
  color: #fff;
}
.pro-slider-box{
  width: 380rpx;
  display: flex;
  justify-content: center;
}
.pro-audio-pross-bg{
  width: 380rpx;
  position: relative;
}
.pro-audio-pross-bg-img{
  width: 380rpx;
  height: 30px;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(0,-50%);
  z-index: 999;
  pointer-events: none;
}
.pro-audio-pross-name{
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
}
.pro-audio-time{
  font-size: 28rpx;
  color: #999;
}
.custom-button{
  background: #ea7152;
  border-radius: 999px;
  padding: 5rpx 10rpx;
  font-size: 24rpx;
  color: white;
}
.van-slider{
  background: url(/images/icons/slider.png) left;
}
/**index.wxss**/
.button{
  position: absolute;
  bottom: 0;
  width: 100%;
}
.aon{
position: absolute;
white-space:nowrap;/* 防止向下换行*/
animation-timing-function: linear;
animation-fill-mode: none;
}
.doommview{
  z-index: 3;
  height: 600rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  pointer-events: none;
  overflow: hidden;
}

@keyframes first{
  from{left: 100%; }
  to{left: -100%;}
}
