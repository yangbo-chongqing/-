/* pages/code/code.wxss */
page{
  padding: 0;
  margin: 0;
  }
.content{
  width: 100%;
  height: 100%;
  background-color:#fff;
  padding-top: 50rpx;
}
.title{
  width: 100%;
  margin-top:20rpx;
  text-align: center;
}
.content_iconfont{
  padding-top: 300rpx;
  text-align: center;
  font-size: 60rpx;
}
.content_iconfont .icon-shouji,.content_iconfont .icon-jiantou_zuoyouqiehuan,.content_iconfont .icon-ico{
  font-size: 100rpx;
  color: #999;
}
.loginword{
  padding-top: 60rpx;
  text-align: center;
  font-size: 50rpx;
  font-weight: 600;
}
.login{
  padding-top: 400rpx;
  display: flex;
  justify-content: center;
}
.login_content{
  display: inline-block;
  width: 300rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #4caf50;
  color: #fff;
  border-radius: 8rpx;
}
.cancel{
  display: flex;
  justify-content: center;
  padding-top: 80rpx;
  color: #999;
}



