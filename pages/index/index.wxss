.home-page {
  padding:20rpx 40rpx;
  box-sizing: border-box;
  width: 100%;
}
.home-page-swiper{
  width: 100%;
  height: 350rpx;
}
.scroll-tab{
  display: flex;
}
.fixedNav{
  position: fixed;
  top: 0;
  left: 0;
  /* border-bottom: 1px solid #ccc; */
  padding:0 40rpx;
  box-shadow: 0px 0px 10px #f5f5f5;
  /* box-sizing: border-box; */
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.scroll-page-title-text{
  flex-shrink: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-right:20rpx;
  color: #999;
  font-size: 30rpx;
}
.scroll-x{
}
.scroll-box{
  background: white;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.scroll-page-title-text.active{
  font-size: 35rpx;
  font-weight: bold;
  color: #000;
}
.home-page-swiper swiper{
  width: 100%;
  height: 350rpx;
  background: white;
}
.find-page-tj-body{
  margin: 40rpx 0;
  display: flex;
  justify-content: space-between;
}
.find-page-tj-body-item{
  width: 325rpx;
  height: 250rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.find-page-tj-body-item1{
  display: flex;
  height: 115rpx;
  display: flex;
  justify-content: space-between;
}
.find-page-tj-body-item1>image{
  width: 100%;
  height: 115rpx;
  border-radius: 10px;
}
.find-page-tj-body-item2{
  width: 151rpx;
}
.find-page-tj-body-item2>image{
  width: 100%;
  height: 115rpx;
  border-radius: 10px;
}
.find-page-tj-body-item>image{
  width: 100%;
  height: 100%;
  border-radius: 10px;
}
.home-page-swiper .swiper-item{
  width: 100%;
  height: 350rpx;
  position: relative;
  background: white;
  overflow: hidden;
}
.home-page-swiper .swiper-item image{
  width: 100%;
  height: 350rpx;
}
.home-page-swiper-name{
  position: absolute;
  bottom: 0;
  left: 0;
  color: white;
  width: 100%;
  height: 100rpx;
  background: linear-gradient(rgba(0,0,0,0),rgba(0,0,0,0.5));
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  font-size: 34rpx;
}

.find-page-title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.find-page-title-text{
  font-size: 35rpx;
  font-weight: bold;
  box-sizing: border-box;
}
.find-page-title-sel{
  display: flex;
  font-size: 32rpx;
  align-items: center;
}
.find-page-title-type{
  display: flex;
  color: #ea7152;
  align-items: center;
}
.find-page-title-more{
  display: flex;
  color: #999999;
  padding: 5rpx 0;
}
.find-page-title-sel-img{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.home-you-sel{
  margin-top: 20rpx;
}
.home-you-sel-item{
  display: flex;
  margin-bottom: 20rpx;
  box-shadow: 0px 0px 10px #f2f2f2;
  border-radius: 10px;
  padding: 20rpx;
  box-sizing: border-box;
}
.home-you-img{
  width: 167rpx;
  height: 167rpx;
  box-sizing: border-box;
  border: 1px solid #ccc;
  border-radius: 10px;
  display: flex;
  align-items: center;
  overflow: hidden;
  box-sizing: border-box;
}
.home-you-img image{
  width: 167rpx;
  height: 167rpx;
}
.home-you-body{
  width: 460rpx;
  margin-left: 20rpx;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.home-you-title{
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
}
.home-you-text{
  font-size: 28rpx;
  color: #999;
  margin-top: 20rpx;
}